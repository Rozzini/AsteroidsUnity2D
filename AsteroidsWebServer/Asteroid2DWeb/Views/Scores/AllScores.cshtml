@model IEnumerable<Asteroid2DWeb.Model>

@{
    ViewData["Title"] = "AllScores";
}


<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js" charset="utf-8"></script>
<link href="~/CSS/PlayerScoreBoard.css" rel="stylesheet" type="text/css" />

@*<h2 style="text-align:center">SCORES</h2>*@
@*<img class="eye-animation"
     src="~/images/AsteroidType2.png"
     alt="blinking eye animation" />*@
<table>
    <h2 style="text-align:center">SCORES</h2>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="padding: 25px 0px 0px 5px;">
                    @item.PlayerName
                </td>
                <td style="padding: 25px 0px 0px 20px;">
                    @item.Score
                </td>
            </tr>
        }
    </tbody>
</table>


