@model IEnumerable<Asteroid2DWeb.Model>

@{
    ViewData["Title"] = "AllScores";
}

<!DOCTYPE html>
<html>
<head>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.0/p5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.0/addons/p5.dom.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.0/addons/p5.sound.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/CSS/PlayerScoreBoard.css">
    <meta charset="utf-8" />

</head>
<body>
    <script src="~/js/AsteroidsMovement.js"></script>

    <table>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="padding: 25px 0px 0px 5px;">
                        @item.PlayerName
                    </td>
                    <td style="padding: 25px 0px 0px 20px;">
                        @item.Score
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>


